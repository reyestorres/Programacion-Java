package actividadesResueltas;

public class Actividad3_15v2 {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		int i;
		int j;
		int numLineas=4;
		for(i=0;i<=numLineas;i++) {
			for(j=1;j<=numLineas-1;j++) {
				System.out.print(" * ");
			}
			System.out.print("\n ");
		}
	}

}
